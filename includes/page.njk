---
layout: main
body: page
---
<header>
{% if hero %}
<div class="hero">
<img src="{{ hero }}">
</div>
{% endif %}
<h1>{{ title }}</h1>
{% if lede %}
<p class="lede">{{ lede }}</p>
{% endif %}
{% if note %}
<p class="note">{{ note | safe }}</p>
{% endif %}
</header>
<article>
{{ content | safe }}
</article>
<footer>
{% include "face.njk" %}
</footer>